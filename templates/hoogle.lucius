ol.search-results {
    margin: 0;
    padding: 0;
    list-style: none;
}

.self {
    margin-bottom: 0;
}

table.sources {
    margin: 0;
    padding: 0;
    font-size: 0.8em;

    th {
        padding-right: 0.5em;
    }

    a, a:visited {
        color: #060;
    }
}

.docs {
    white-space: pre-wrap;
    background: #f8f8f8;
}

.docs, .nodocs {
    margin-left: 1em;
    padding: 0.5em;
}

.nodocs {
    font-style: italic;
}

.exact-lookup {
  display: inline-block;
  margin-left: 0.5em;
}
input[type=submit] {
  margin-bottom: 10px;
}

.haddocks {
    font-weight: bold;
    margin-bottom: 1em;

    ul {
        display: inline;
        padding: 0;
        margin: 0;
    }
    li {
        display: inline-block;
        font-weight: normal;
        margin-left: 1em;
    }
}
